{
  "name": "@alieutech/seedsmith",
  "version": "0.1.2",
  "description": "CLI + library to auto-generate MongoDB seed data from Mongoose schemas",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "seedsmith": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "start": "node dist/cli.js",
    "test": "jest --config jest.config.cjs --runInBand",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "seed",
    "mongodb",
    "mongoose",
    "faker",
    "cli",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "mongoose": "^8.5.2"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^25.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.12",
    "mongodb-memory-server": "^10.1.4"
  }
}
